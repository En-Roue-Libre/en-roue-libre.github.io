<h3 class="text-xl font-sans text-lime-100 mb-2">Tanks</h3>
{%- for tank in event.tanks -%}
    <div class="flex items-center space-x-4 mb-2">
        <img class="w-6 h-6 rounded" src="{{ tank.iconUrl }}" alt="{{ tank.specName }}">
        <div class="font-medium text-white">
            <div class="capitalize text-{{ tank.classColor }}">{{ tank.name }}</div>
        </div>
    </div>
{%- endfor -%}